{
  "name": "water-abstraction-returns",
  "version": "1.24.1",
  "description": "Service for water abstraction returns data",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/DEFRA/water-abstraction-returns"
  },
  "scripts": {
    "test": "lab",
    "test:travis": "lab -t 55 -m 0 --coverage-path ./src/ -r lcov -o coverage/lcov.info -r console -o stdout",
    "test-cov-html": "lab -r html -o coverage.html",
    "migrate": "node scripts/create-schema && db-migrate up --verbose",
    "migrate:down": "db-migrate down --verbose",
    "migrate:create": "db-migrate create --sql-file --",
    "lint": "eslint .",
    "codecov": "codecov",
    "version": "auto-changelog -p --commit-limit false && git add CHANGELOG.md"
  },
  "engines": {
    "node": ">=8.0"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "auto-changelog": "^1.13.0",
    "code": "^5.2.4",
    "codecov": "^3.5.0",
    "db-migrate": "^0.11.5",
    "db-migrate-pg": "^0.5.1",
    "eslint": "5.16.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "9.0.1",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-standard": "4.0.0",
    "lab": "^15.5.0",
    "sinon": "^7.3.2"
  },
  "dependencies": {
    "@envage/hapi-pg-rest-api": "^4.1.1",
    "@envage/water-abstraction-helpers": "^3.0.0",
    "blipp": "^3.0.0",
    "boom": "^5.2.0",
    "dotenv": "^4.0.0",
    "good": "^8.1.2",
    "good-winston": "^4.0.0",
    "hapi": "^17.6.0",
    "hapi-auth-jwt2": "^8.6.1",
    "hoek": "^4.2.1",
    "joi": "^13.7.0",
    "knox": "^0.9.2",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "pg": "^7.11.0",
    "request": "^2.88.0"
  }
}
